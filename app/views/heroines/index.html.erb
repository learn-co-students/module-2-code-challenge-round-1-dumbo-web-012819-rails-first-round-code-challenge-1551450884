<h1>All Heroines</h1>

<h3><%= link_to "Create Heroine", new_heroine_path %></h3>

<h4>Search By Superpower:</h4>
<%= form_tag(heroines_path, method: :get) do %>
  <%= text_field_tag(:search, params[:search]) %>
  <%= submit_tag 'Search' %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine %>
    </li>
    <br>
  <% end %>
</ul>
